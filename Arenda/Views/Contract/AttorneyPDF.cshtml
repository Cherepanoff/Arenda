@model IEnumerable<Arenda.Models.PdfFile>
<head>
    <meta charset="utf-8" />
    <link rel="stylesheet" href="~/css/style.css" />
</head>
<body>
    <h2>Доверенность</h2>
    @foreach (var m in Model)
    {
        @if (m.Pdfattorney != null)
        {
            <a class="btn" href="@m.Pdfattorney" target="_blank">@m.Pdfattorney</a>
        }
    }
    <form asp-controller="Contract" asp-action="UploadAttorneyPDF" asp-route-id=@TempData["ID"]
          enctype="multipart/form-data" method="post">
        <input type="file" name="AttorneyPDF" />
        <button type="submit" class="btn">Загрузить договор</button>
    </form>
</body>