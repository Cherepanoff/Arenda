@model IEnumerable<Arenda.Models.PdfFile>
<head>
    <meta charset="utf-8" />
    <link rel="stylesheet" href="~/css/style.css" />
</head>
<body>
    <h2>Учредительные документы</h2>
    @foreach (var m in Model)
    {
        @if (m.Pdfdoc != null)
        {
            <a class="btn" href="@m.Pdfdoc" target="_blank">@m.Pdfdoc</a>
        }
    }
    <form asp-controller="Contract" asp-action="UploadDocPDF" asp-route-id=@TempData["ID"]
          enctype="multipart/form-data" method="post">
        <input type="file" name="DOCPDF" />
        <button type="submit" class="btn">Загрузить договор</button>
    </form>
</body>
